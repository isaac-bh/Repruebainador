{
  "name": "Repruebainador",
  "version": "1.0.0",
  "description": "Release the grades.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . --out=dist/win --platform=win32 --arch=x64 --icon=Archivos/Imagenes/Milos.ico --asar=true --overwrite",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "",
  "keywords": [
    "Examenes",
    "Visión Artificial",
    "Python",
    "Electron",
    "Polinova"
  ],
  "author": "Isaac Benavides, Isaac Lomelí y Joshua Romo.",
  "license": "MIT",
  "build": {
    "appId": "com.repruebainador.app",
    "asar": true,
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  },
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-builder": "^22.5.1"
  },
  "dependencies": {
    "python-shell": "^1.0.8",
    "xmlhttprequest": "^1.8.0"
  }
}
